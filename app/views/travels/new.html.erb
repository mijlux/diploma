<div class="container">
  <h2 class="title">Travel create</h2>

  <div class="mt">
    <%=form_with model: @travel, data: {turbo: false} do |form| %>
      <p class = "mt"><%= form.label :country, 'Введите country' %> </p>
      <p><%= form.text_field :country, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :city, 'Введите city' %> </p>
      <p><%= form.text_field :city, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :climate, 'Введите climate' %> </p>
      <p><%= form.text_field :climate, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :iswater, 'Введите iswater' %> </p>
      <p><%= form.text_field :iswater, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :iswmount, 'Введите ismount' %> </p>
      <p><%= form.text_field :ismount, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :ispopular, 'Введите ispopular' %> </p>
      <p><%= form.text_field :ispopular, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :description, 'Введите descr' %> </p>
      <p><%= form.text_field :description, class: 'form-text-field' %> </p>

      <p class = "mt"><%= form.label :city_image, 'Введите city_image' %> </p>
      <p><%= form.file_field :city_image, class: 'form-file-field' %> </p>



      <p class = "mt-lg"><%= form.submit 'Save', class: 'button button-main' %></p>
    <% end %>
  </div>
</div>
